{
  "android": {
    "appActivity": "com.zoho.accounts.oneauth.v2.ui.login.WalkthroughActivity",
    "appPackage": "com.zoho.accounts.oneauth",
    "automationName": "UiAutomator2",
    "noReset": false,
    "platformName": "android",
    "uiautomator2ServerInstallTimeout": 120000
  },
  "serverConfig" : {
    "server": {
      "use-plugins": ["device-farm", "element-wait", "appium-dashboard"],
      "plugin": {
        "device-farm": {
          "platform": "android",
          "skipChromeDownload": true
        }
      }
    }
  },
  "appiumServerPath": "/opt/homebrew/lib/node_modules/appium/build/lib/main.js"
}
